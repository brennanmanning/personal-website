---
import BaseLayout from '../../layouts/BaseLayout.astro'
const articles = Astro.fetchContent("./*.md")
console.log(articles)
---

<BaseLayout title="Blog" name="Blog">
  <div>
    {articles.map((post) => (
      <article>
        <a href={post.url}> {post.title}</a>
        <p> {post.date}</p>
      </article>
    ))}
  </div>
</BaseLayout>
